{
  "Serilog": {
    "Using": [
      "Serilog.Sinks.Graylog",
      "Serilog.Sinks.Console"
    ],
    "MinimumLevel": {
      "Default": "Information"
    },
    "WriteTo": [
      {
        "Name": "Console"
      },
      {
        "Name": "Async",
        "Args": {
          "configure": [
            {
              "Name": "Graylog",
              "Args": {
                "hostnameOrAddress": "",
                "port": "12201",
                "transportType": "Http",
                "minimumLogEventLevel": "Warning",
                "useSecureConnection": false,
                "additionalFields": {
                  "ApplicationName": "CleanArchitecture"
                }
              }
            }
          ],
          "bufferSize": 1000,
          "flushInterval": "00:00:02"
        }
      }
    ],
    "Enrich": [
      "FromLogContext",
      "WithMachineName",
      "WithThreadId"
    ]
  },
  "EmailSettings": {
    "SenderDisplayName": "CleanArchitecture",
    "SenderEmail": "",
    "SmtpServer": "",
    "SmtpPort": 25
  },
  "ConnectionStrings": {
    "Database": "",
    "Redis": ""
  },
  "CacheSettings": {
    "Prefix": "CleanArchitecture_",
    "TimeToLiveInMilliSeconds": 10000
  },
  "AuthenticationSettings": [
    {
      "Name": "AzureAD",
      "MetadataAddress": "",
      "Authority": "",
      "Audience": "api://f3dc9bce-396d-4b2f-b343-324830343ea9"
    },
    {
      "Name": "KeyCloakAD",
      "MetadataAddress": "",
      "Authority": "",
      "Audience": "account"
    }
  ],
  "AllowedHosts": "*"
}